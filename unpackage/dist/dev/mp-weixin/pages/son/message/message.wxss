.father {
  overflow: hidden;
  position: relative;
  height: 100vh;
  background-image: linear-gradient(rgba(171, 211, 218, 0.7), rgba(234, 217, 178, 0.7)), url(data:image/jpeg;base64,);
  background-size: 100%;
  background-repeat: no-repeat;
}
.title {
  display: flex;
  justify-content: center;
}
.icon {
  position: fixed;
  right: 0;
  display: flex;
  align-self: flex-end;
